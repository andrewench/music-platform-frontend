{
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": false,
	"bracketSameLine": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"(.*react.*)",
		"^@/components/global",
		"^@/components/layout",
		"^@/components/ui",
		"^@/components/screens",
		"^@/components/views",
		"^@/data",
		"^(.*.package.json)$",
		"^(./App)$",
		"^@/",
		"^@/styles/*",
		"^(.*.module.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
