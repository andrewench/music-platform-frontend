{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "importOrder": [
    "(.*react.*)",
    "<THIRD_PARTY_MODULES>",
    "^@@/",
    "^@/components/global",
    "^@/components/layout",
    "^@/components/ui",
    "^@/components/screens",
    "^@/components/shared",
    "^@/components/views",
    "^@/components/providers",
    "^@/store",
    "^@/store/slices",
    "^@/shared/api",
    "^@/shared/data",
    "^@/shared/languages",
    "^@/shared/schemes",
    "^@/shared/patterns",
    "^@/shared/hooks",
    "^@/shared/utils",
    "^@/shared/types",
    "^(.*.component)$",
    "^(.*.interface)$",
    "^(.*.type)$",
    "^(.*.package.json)$",
    "^@/",
    "^@/assets/styles/*",
    "^(.*.module.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
