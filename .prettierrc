{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "importOrder": [
    "(.*react.*)",
    "<THIRD_PARTY_MODULES>",
    "^@/components/layout",
    "^@/components/interface",
    "^@/components/ui",
    "^@/components/icons",
    "^@/components/screens",
    "^@/components/shared",
    "^@/components/views",
    "^@/components/providers",
    "^@/components/guards",
    "^@/store",
    "^@/guards",
    "^@/services",
    "^@/store/slices",
    "^@/shared/api",
    "^@/shared/schemes",
    "^@/shared/presets",
    "^@/shared/constants",
    "^@/shared/hooks",
    "^@/shared/utils",
    "^@/shared/types",
    "^(.*.component)$",
    "^(.*.provider)$",
    "^(.*.interface)$",
    "^(.*.data)$",
    "^(.*.constants)$",
    "^(.*.hook)$",
    "^(.*.type)$",
    "^@/",
    "^@/assets/styles/*",
    "^(.*.module.scss)$"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
